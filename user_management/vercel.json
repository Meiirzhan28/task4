{
    "version": 3,
    "builds":[{
        "src":"user_management/wsgi.py",
        "use":"@vercel/python",
        "config":{"maxLambdaSize":"15mb","runtime":"python3.9"}
    },
    {
        "src":"build_files.sh",
        "use":"@vercel/static-build",
        "config":{"distDir":"staticfiles_build"}
    }],
    "routes":[{
        "src":"/(.*)",
        "dest":"user_management/wsgi.py"
    }]
}